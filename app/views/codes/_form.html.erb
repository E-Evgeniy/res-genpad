
  <%= form_with model: @code, local: true do |form| %>
  <%= render 'shared/errors', resource: @code %>
  <p>
    <%= form.label :code %><br>
    <%= form.text_field :code, id: :code_code%>
  </p>
  <p>
    <%= form.label :threshold %><br>
    <%= form.text_field :threshold, id: :code_threshold %>
  </p>

  <br>

  <input type="submit" value="Edit">
  <% end %>
