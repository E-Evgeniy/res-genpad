<h2> Поиск </h2>
<%= form_tag("result", method: "get") do %>
  <p>
  <%= label_tag(:data_begin, "Введите начальную дату") %>
  <%= date_field_tag(:data_begin) %>
  </p>
  <p>
  <%= label_tag(:data_end, "Введите конечную дату") %>
  <%= date_field_tag(:data_end) %>
  </p>
  <p><%= submit_tag("Search") %></p>
<% end %>